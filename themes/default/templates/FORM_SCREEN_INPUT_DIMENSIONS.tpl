<div class="input_dimensions">
	<input maxlength="6" data-cms-invalid-pattern="[^\-\d&#92;{$DECIMAL_POINT*}]" tabindex="{TABINDEX*}" class="input_integer{REQUIRED*}" type="number" id="{NAME_WIDTH*}" name="{NAME_WIDTH*}" value="{DEFAULT_WIDTH*}" />
	&times;
	<label for="{NAME_HEIGHT*}" class="accessibility_hidden">{!HEIGHT}</label>
	<input maxlength="6" data-cms-invalid-pattern="[^\-\d&#92;{$DECIMAL_POINT*}]" tabindex="{TABINDEX*}" class="input_integer{REQUIRED*}" type="number" id="{NAME_HEIGHT*}" name="{NAME_HEIGHT*}" value="{DEFAULT_HEIGHT*}" />
</div>
